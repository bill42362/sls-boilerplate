{
  "name": "sls-boilerplate",
  "version": "1.7.4",
  "description": "Serverless boilerplate with express server, auto test and web document.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bill42362/sls-boilerplate.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "buildjs": "cross-env NODE_ENV=production babel src -d dist --ignore .test.js",
    "build": "npm run clean && npm run buildjs && npm run document:build",
    "deploy": "npm run build && sls deploy -v",
    "start": "npm run build && node dist",
    "start:auto": "nodemon --ignore dist --exec babel-node src",
    "test": "cross-env NODE_ENV=aws babel-node src/test.js",
    "devtest": "npm run build && node dist/test.js",
    "devtest:auto": "nodemon --ignore dist --exec babel-node src/test.js",
    "document:build": "rimraf dist/document && cross-env NODE_ENV=production webpack -p --progress",
    "document:wds": "webpack-dev-server --progress",
    "document:devserver": "nodemon --ignore dist --ignore src/document/ --exec babel-node src/document.js",
    "document:start": "npm run build && cross-env NODE_ENV=production node dist/document.js"
  },
  "keywords": [
    "serverless",
    "boilerplate",
    "express",
    "test"
  ],
  "author": "Pi-Yin Hsiao",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "colors": "^1.1.2",
    "cors": "^2.8.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "deep-freeze": "0.0.1",
    "diff": "^3.3.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "graceful-fs": "^4.1.11",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.9.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-json-pretty": "^1.6.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta"
  }
}
