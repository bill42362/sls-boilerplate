{
  "name": "sls-boilerplate",
  "version": "1.0.0",
  "description": "Serverless boilerplate with express server, auto test and web document.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bill42362/sls-boilerplate.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "buildjs": "cross-env NODE_ENV=production babel src -d dist --ignore .test.js",
    "build": "npm run clean && npm run buildjs",
    "deploy": "npm run build && sls deploy -v",
    "start": "nodemon --ignore dist --exec babel-node src",
    "test": "cross-env NODE_ENV=aws babel-node src/test.js",
    "devtest": "nodemon --ignore dist --exec babel-node src/test.js"
  },
  "keywords": [
    "serverless",
    "boilerplate",
    "express",
    "test"
  ],
  "author": "Pi-Yin Hsiao",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.17.2",
    "colors": "^1.1.2",
    "cross-env": "^5.0.5",
    "diff": "^3.3.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.9.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  }
}
