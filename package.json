{
  "name": "sls-boilerplate",
  "version": "1.0.0",
  "description": "Serverless boilerplate with express server, auto test and web document.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bill42362/sls-boilerplate.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "rmdist": "rimraf dist",
    "buildjs": "babel src -d dist --ignore .test.js",
    "build": "npm run rmdist && npm run buildjs",
    "deploy": "npm run build && sls deploy -v",
    "test": "jest"
  },
  "keywords": [
    "serverless",
    "boilerplate",
    "express",
    "test"
  ],
  "author": "Pi-Yin Hsiao",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.15.4",
    "rimraf": "^2.6.1"
  }
}
